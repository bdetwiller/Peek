<a class="fancybox" 
	  rel="group"
	  <% if(photo.get("caption")) { %>
	    title="<b>@<%= photo.get("user").username%> -</b> <%=$ .timeago(new Date(photo.get("created_time") * 1000)) %>: <br><%= photo.get("caption").text%>"
	  <% } else { %>
	  	title="<b>@<%= photo.get("user").username%> -</b> <%=$ .timeago(new Date(photo.get("created_time") * 1000)) %>"
	  <% } %>
	  href="<%=photo.get("images").standard_resolution.url %>" >
	   <img src="<%=photo.get("images").thumbnail.url%>" alt="" />
	</a>
